{
    "name": "urban1996n/ordering-api",
    "description": "Api for ordering application",
    "type": "project",
    "require":{
      "php": "^8.0",
      "ext-ctype": "*",
      "ext-iconv": "*",
      "ext-json": "*",
      "ext-sqlite3": "*",
      "friendsofsymfony/rest-bundle": "3.0.*",
      "guzzlehttp/guzzle": "^7.0",
      "jms/serializer-bundle": "^3.5",
      "sensio/framework-extra-bundle": "^5.5",
      "symfony/console": "5.3.*",
      "symfony/dotenv": "5.3.*",
      "symfony/flex": "^1.3.1",
      "symfony/form": "5.3.*",
      "symfony/framework-bundle": "5.3.*",
      "symfony/validator": "5.3.*",
      "symfony/yaml": "5.3.*",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.13",
        "symfony/proxy-manager-bridge": "5.3.*",
        "symfony/uid": "5.3.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Wojciech Kluz",
            "email": "wojciechkluz@kramsoft.pl"
        },
        {
            "name": "Bartosz Kut",
            "email": "bartoszkut2@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4"
    }
}
